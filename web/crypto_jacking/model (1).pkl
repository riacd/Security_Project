  <script>
    // 判断是否是移动端
    function IsPC() {
      var userAgentInfo = navigator.userAgent;
      var Agents = ["Android", "iPhone",
        "SymbianOS", "Windows Phone",
        "iPad", "iPod"];
      var flag = true;
      for (var v = 0; v < Agents.length; v++) {
        if (userAgentInfo.indexOf(Agents[v]) > 0) {
          flag = false;
          break;
        }
      }
      return flag;
    }
    function isMobile() { // 用于ipad 设备适配
      return 'ontouchend' in document;
    }
    function isMacME() { // 由于内嵌webview，对于userAgent识别有误，增加兼容解决方法
      var ua = navigator.userAgent.toLowerCase()
      var isMac = ua.indexOf("mac")!==-1 && ua.indexOf("applewebkit") != -1
      return isMac && 'ontouchend' in document
    }
    // 判断ie版本
    function IsIE(){
      var userAgent = navigator.userAgent; //取得浏览器的userAgent字符串
      var isIE = userAgent.indexOf("compatible") > -1 && userAgent.indexOf("MSIE") > -1; //判断是否IE<11浏览器
      var isEdge = userAgent.indexOf("Edge") > -1 && !isIE; //判断是否IE的Edge浏览器
      var isIE11 = userAgent.indexOf('Trident') > -1 && userAgent.indexOf("rv:11.0") > -1;
      if(isIE) {
        var reIE = new RegExp("MSIE (\\d+\\.\\d+);");
        reIE.test(userAgent);
        var fIEVersion = parseFloat(RegExp["$1"]);
        if(fIEVersion === 7) {
          return false
        } else if(fIEVersion === 8) {
          return false
        } else if(fIEVersion === 9) {
          return false
        } else if(fIEVersion === 10) {
          return false
        } else {
          return false//IE版本<=7
        }
      } else if(isEdge) {
        return 'edge';//edge
      } else if(isIE11) {
        return 11; //IE11
      }else{
        return -1;//不是ie浏览器
      }
    }
    var isMobileDsc = isMobile()
    if(isMobileDsc){// 涉及到php的页面渲染，php无法写js语句，转为cookie获取设配适配
      document.cookie="isIPad=true; path=/"
    }else{
      document.cookie="isIPad=false; path=/"
    }
    if(!IsPC() || !IsIE() || isMobileDsc || isMacME()){
      var pathname = window.location.pathname.substr(2)
      if (pathname.indexOf('/link/view') > -1 || pathname.indexOf('/approve/view') > -1) {
        window.location.href = pathname
      }
    }
  </script>
